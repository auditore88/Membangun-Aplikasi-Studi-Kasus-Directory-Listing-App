<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Places</title>
</head>
<body>
    <!-- show places from places variables -->
    <h1>Places</h1>
    <a href="/places/create">Add Place</a>
    <ul>
        <% for(const place of places) { %>
            <li> <a href="/places/<%= place._id %>">  <%= place.title %> </a></li>
        <% } %>
    </ul>
</body>
</html>